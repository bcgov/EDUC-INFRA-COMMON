{
  "name": "github-tools",
  "description": "Commandline tools for Github",
  "version": "1.0.0",
  "author": "Mingwei",
  "bin": {
    "github-tools": "./bin/run"
  },
  "bugs": "https://github.com/bcgov/EDUC-PEN-DEMOG-API/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "@octokit/rest": "^17.8.0",
    "tweetsodium": "0.0.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2",
    "rimraf": "^3.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/bcgov/EDUC-PEN-DEMOG-API",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "github-tools",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bcgov/EDUC-PEN-DEMOG-API",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
